file(READ ${@GIT_PREFIX@_GIT_VERSION_FILE} @GIT_PREFIX@_VERSION_TAG_RAW LIMIT 1024)
string(STRIP "${@GIT_PREFIX@_VERSION_TAG_RAW}" @GIT_PREFIX@_VERSION)

configure_file(${@GIT_PREFIX@_VERSION_FILE_TEMPLATE} ${@GIT_PREFIX@_VERSION_FILE})
message("Building @GIT_PREFIX@ version ${@GIT_PREFIX@_VERSION}")
